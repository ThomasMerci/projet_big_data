# ranger.env

# Specify the Ranger admin username and password
RANGER_ADMIN_USERNAME=admin
RANGER_ADMIN_PASSWORD=admin

# Specify the database type (e.g., "mysql", "postgresql", etc.)
RANGER_DB_FLAVOR=mysql

# Specify the hostname or IP address of the Ranger database server
RANGER_DB_HOST=ranger-db

# Specify the database port (default is usually fine)
RANGER_DB_PORT=3306

# Specify the name of the Ranger database
RANGER_DB_NAME=ranger

# Specify the Ranger database username and password
RANGER_DB_USER=ranger
RANGER_DB_PASSWORD=ranger

# Specify the data directory for Ranger
RANGER_DATA_DIR=/opt/ranger/data

